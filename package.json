{
  "name": "nightlife",
  "version": "1.0.0",
  "description": "full stack project from FCC",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server/server.js",
    "dev": "start http://localhost:8080/ & webpack-dev-server --config ./webpack.config.js --mode development",
    "build": "webpack -p --progress",
    "dev-server": "concurrently -k \"npm start\" \"npm run dev\"",
    "test-watch": "nodemon --exec \"npm test\"",
    "test": "NODE_ENV=test || SET \"NODE_ENV=test\" && mocha server/tests/test.js --exit"
  },
