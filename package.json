{
	"name": "nightlife",
	"version": "1.0.0",
	"description": "app that shows bars and clubs in searchable area",
	"main": "server.js",
	"scripts": {
		"dev:start": "concurrently -k \"npm run server\" \"npm run dev\"",
		"dev": "webpack serve --open http://localhost:8080/ & webpack-dev-server --config ./webpack.config.js",
		"server": "nodemon -r dotenv/config server/server.js",
		"env": "env $(cat .env | grep -v \"#\" | xargs) npm run server",
		"build": "webpack -p --progress",
		"test-watch": "nodemon --exec \"npm test\"",
		"test": "SET \"NODE_ENV=test\" && mocha server/**/*.test.js",
		"start": "node server/server.js",
		"heroku-postbuild": "npm run build"
	},
	"author": "Steve Failla",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/Stevo99/nightlife.git"
	},
	"proxy": "http://localhost:3001",
	"engines": {
		"node": "10.15.1",
		"npm": "6.4.1"
	},
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.18.3",
		"concurrently": "^3.6.1",
		"cors": "^2.8.4",
		"express": "^4.17.3",
		"helmet": "^3.23.1",
		"jsonwebtoken": "^8.3.0",
		"jwt-decode": "^2.2.0",
		"mongoose": "^5.7.5",
		"node-sass": "^4.14.1",
		"npm": "^7.0.3",
		"path": "^0.12.7",
		"react": "^16.4.2",
		"react-dom": "^16.4.2",
		"react-hamburger-menu": "^1.1.1",
		"react-redux": "^5.0.7",
		"redux": "^4.0.0",
		"redux-thunk": "^2.3.0"
	},
	"devDependencies": {
		"@babel/core": "^7.12.9",
		"@babel/plugin-proposal-class-properties": "^7.12.1",
		"@babel/plugin-proposal-object-rest-spread": "^7.12.1",
		"@babel/plugin-transform-runtime": "^7.12.1",
		"@babel/preset-env": "^7.12.7",
		"@babel/preset-react": "^7.12.7",
		"babel-loader": "^8.2.2",
		"css-hot-loader": "^1.4.2",
		"css-loader": "^3.1.0",
		"dotenv": "^8.2.0",
		"dotenv-webpack": "^1.8.0",
		"expect": "^24.1.0",
		"file-loader": "^1.1.11",
		"html-loader": "^0.5.5",
		"html-webpack-plugin": "^4.5.0",
		"image-webpack-loader": "^4.5.0",
		"mini-css-extract-plugin": "^1.3.2",
		"mocha": "^8.2.0",
		"morgan": "^1.9.0",
		"nodemon": "^2.0.4",
		"prop-types": "^15.6.2",
		"react-router-dom": "^4.3.1",
		"request": "^2.88.0",
		"sass-loader": "^7.1.0",
		"style-loader": "^0.21.0",
		"supertest": "^3.2.0",
		"url-loader": "^1.1.2",
		"webpack": "^5.10.0",
